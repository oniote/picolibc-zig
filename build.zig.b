            if (newlib.rootModuleTarget().cpu.arch == .x86_64) {
                newlib.addAssemblyFile(pico_path(b, "newlib/libc/machine/x86/memcpy-64.S"));
                newlib.addAssemblyFile(pico_path(b, "newlib/libc/machine/x86/memset-64.S"));
                newlib.addAssemblyFile(pico_path(b, "newlib/libc/machine/x86/setjmp-64.S"));
                newlib.addAssemblyFile(pico_path(b, "picocrt/machine/x86/crt0-64.S"));
