            newlib.addAssemblyFile(pico_path(b, "newlib/libc/machine/arc/memcpy.S"));
            newlib.addAssemblyFile(pico_path(b, "newlib/libc/machine/arc/memset-archs.S"));
            newlib.addAssemblyFile(pico_path(b, "newlib/libc/machine/arc/memset-bs.S"));
            newlib.addAssemblyFile(pico_path(b, "newlib/libc/machine/arc/memset.S"));
            newlib.addAssemblyFile(pico_path(b, "newlib/libc/machine/arc/setjmp.S"));
            newlib.addAssemblyFile(pico_path(b, "newlib/libc/machine/arc/strchr-bs-norm.S"));
            newlib.addAssemblyFile(pico_path(b, "newlib/libc/machine/arc/strchr-bs.S"));
            newlib.addAssemblyFile(pico_path(b, "newlib/libc/machine/arc/strchr.S"));
            newlib.addAssemblyFile(pico_path(b, "newlib/libc/machine/arc/strcmp-archs.S"));
            newlib.addAssemblyFile(pico_path(b, "newlib/libc/machine/arc/strcmp.S"));
            newlib.addAssemblyFile(pico_path(b, "newlib/libc/machine/arc/strcpy-bs-arc600.S"));
            newlib.addAssemblyFile(pico_path(b, "newlib/libc/machine/arc/strcpy-bs.S"));
            newlib.addAssemblyFile(pico_path(b, "newlib/libc/machine/arc/strcpy.S"));
            newlib.addAssemblyFile(pico_path(b, "newlib/libc/machine/arc/strlen-bs-norm.S"));
            newlib.addAssemblyFile(pico_path(b, "newlib/libc/machine/arc/strlen-bs.S"));
            newlib.addAssemblyFile(pico_path(b, "newlib/libc/machine/arc/strlen.S"));
            newlib.addAssemblyFile(pico_path(b, "newlib/libc/machine/arc/strncpy-bs.S"));
