    const newlib = b.addStaticLibrary(.{
        .name = "c",
        .target = target,
        .optimize = optimize,
    });
    newlib.addConfigHeader(picolibc_h);
    newlib.addConfigHeader(reent_h);

    newlib.addSystemIncludePath(pico_path(b, "newlib/libc/include"));
    newlib.addSystemIncludePath(pico_path(b, "newlib/libm/common"));
    newlib.addSystemIncludePath(pico_path(b, "newlib/libc/stdio"));
    newlib.addSystemIncludePath(pico_path(b, "newlib/libc/stdio/sys"));
    newlib.defineCMacro("_LIBC", null);
    newlib.defineCMacro("TZNAME_MAX", "10");
    switch (newlib.rootModuleTarget().cpu.arch) {
        .avr => {
            newlib.addAssemblyFile(pico_path(b, "newlib/libc/machine/avr/setjmp.S"));
        },
        .arc => {
            newlib.addSystemIncludePath(pico_path(b, "newlib/libc/machine/arc"));
