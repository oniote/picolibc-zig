const std = @import("std");

const module_name = "picolibc";

fn pico_path(b: *std.Build, comptime sub_path: []const u8) std.Build.LazyPath {
    return b.path(module_name ++ "/" ++ sub_path);
}

pub fn build(b: *std.Build) !void {
    const picolibc_ver = try std.SemanticVersion.parse("1.8.6");
    const newlib_ver = try std.SemanticVersion.parse("4.3.0");

    const target = b.standardTargetOptions(.{});
    const optimize = b.standardOptimizeOption(.{});
    const reent_h = b.addConfigHeader(.{
        .include_path = "reent.h",
        .style = .blank,
    }, .{});
    const picolibc_h = b.addConfigHeader(.{
        .include_path = "picolibc.h",
        .style = .{
            .cmake = pico_path(b, "picolibc.h.in"),
        },
